{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "target": "es2015",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@core": [
        "./src/app/@core"
      ],
      "@core/*": [
        "./src/app/@core/*"
      ],
      "@shared": [
        "./src/app/@shared"
      ],
      "@shared/*": [
        "./src/app/@shared/*"
      ],
      "@env/*": [
        "./src/environments/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "../node_modules/@types/node/globals.d.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
