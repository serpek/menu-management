{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./apps/client",
    "outDir": "dist/client",
    "types": [],
    "target": "es2015",
    "paths": {
      "@app/*": [
        "./src/app/*"
      ],
      "@core": [
        "./src/app/@core"
      ],
      "@core/*": [
        "./src/app/@core/*"
      ],
      "@shared": [
        "./src/app/@shared"
      ],
      "@shared/*": [
        "./src/app/@shared/*"
      ],
      "@env/*": [
        "./src/environments/*"
      ]
    }
  },
  "include": ["apps/client/src/**/*.d.ts"],
  "files": [
    "./apps/client/src/main.ts",
    "./apps/client/src/polyfills.ts"
  ]
}
